{
  "name": "coolify-github-bot",
  "version": "1.0.0",
  "description": "GitHub Action for posting Coolify deployment status to PRs",
  "main": "dist/index.js",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node",
    "package": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "test": "bun run src/test.ts",
    "dev": "bun run src/index.ts"
  },
  "keywords": [
    "github-action",
    "coolify",
    "deployment",
    "ci-cd"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vercel/ncc": "^0.38.1",
    "typescript": "^5.3.3"
  }
}

